  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^2.2.3",
    "@cypress/vue": "^3.1.2",
    "@vitejs/plugin-vue": "^2.3.3",
    "cypress": "^9.7.0",
    "start-server-and-test": "^1.14.0",
    "vite": "^2.9.9"
  }
}
